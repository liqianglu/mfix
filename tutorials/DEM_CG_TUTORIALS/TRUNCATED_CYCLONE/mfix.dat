  NORM_G = 0.0
  NORM_S = 0.0

!===============================================================================
! Cartesian Grid - Get geometry from .stl file
!===============================================================================
  CARTESIAN_GRID = .TRUE.

  USE_STL = .TRUE.

  SCALE_STL = 11.73
                      

  TX_STL = 1.1
  TY_STL = 0.3
  TZ_STL = 0.5
  MAX_FACETS_PER_CELL_DES=30
