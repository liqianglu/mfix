  IC_X_e(1)             =  4.   1.62 4. 
  IC_Y_s(1)             =  0.   1.16  0.0
  IC_Y_n(1)             =  3.2  2.35  2.0
  IC_Z_b(1)             =  0.0  0.0    0.                   
  IC_Z_t(1)             =  4.   1.09   1.09                   
 
                                                        !initial values in the region
  IC_EP_g(1)            =  1.  0.9   1.0                     !void fraction 
  IC_ROP_s(1,1)         =  0.  0.2   0.0                     !void fraction  
!  IC_ROP_s(1,2)         =  0.1  0.1   0.                     !void fraction  
  IC_U_g(1)             =  0.0  0.0   0.                  !x gas velocity
  IC_V_g(1)             = 100.0  100. 100.                      !y gas velocity
  IC_W_g(1)             =  0.0   0.    0.                   !z gas velocity
  IC_U_s(1,1)           =  0.0   10.   0.                   !x solids velocity 
  IC_V_s(1,1)           =  10.  0.     0.                   !y solids velocity
  IC_W_s(1,1)           =  0.0   0.    0.                   !z solids velocity
  IC_theta_m(1,1)       =  1.0  0.d0   0.
!  IC_U_s(1,2)          =  0.0   0.    0.                   !x solids velocity 
!  IC_V_s(1,2)          =  10.0  -10.  0.                   !y solids velocity
!  IC_W_s(1,2)          =  0.0   0.    0.                   !z solids velocity
!  IC_theta_m(1,2)      =  1.0  1.0   0.
  IC_DES_FIT_TO_REGION(1) = .f. .t. .f.


!  Boundary Conditions Section                                                  

       ! 1. Distributor flow                                       
  BC_X_w(1)             =  0.0                          !gas distributor plate
  BC_X_e(1)             =  4.0 
  BC_Y_s(1)             =  0.0 
  BC_Y_n(1)             =  0.0 
  BC_Z_b(1)             =  0.0 
  BC_Z_t(1)             =  4.0


  BC_TYPE(1)            = 'MI'                          !specified mass inflow 

  BC_EP_g(1)            =   0.98 
  BC_U_g(1)             =   0.0 
  BC_V_g(1)             = 100.0 
  BC_W_g(1)             =   0.0
  BC_U_s(1,1)           =   0.0
  BC_V_s(1,1)           = 100.0
  BC_W_s(1,1)           =   0.0
  BC_P_g(1)             =   0.0                  

                                           
       ! 2. Exit
  BC_X_w(2)             =   0.0                         !top exit
  BC_X_e(2)             =   4.0                         
  BC_Y_s(2)             =  3.2
  BC_Y_n(2)             =  3.2
  BC_Z_b(2)             =   0.0
  BC_Z_t(2)             =   4.0

  BC_TYPE(2)            =  'PO'                         !specified pressure outflow
  BC_P_g(2)             =  0.0                                 
gravity = 490.0                                              
 DISCRETE_ELEMENT = .t. 
 DIMN  = 3
  DES_CONTINUUM_COUPLED = .false.
  DES_INTERP_ON         = .F.
  NFACTOR               = 200
  WALLDTSPLIT           = .TRUE.
  DRAG_TYPE             = 'GIDASPOW'
  DES_INTERP_MEAN_FIELDS = .True.
  MPPIC = .f.	

!  PVEL_StDev = 1.d0
!  pvel_mean = -5.d0
  MAX_PIS = 400000
  PARTICLES             = 1    ! Number of particles
!  PARTICLES             = 380    ! Number of particles

      DTSOLID_FACTOR = 1.d0 
  PRINT_DES_DATA = .t.		

  GENER_PART_CONFIG = .t.
  !des_eps_xstart  = 1.
  DES_NEIGHBOR_SEARCH   = 4        ! Particle neighbor search method; 1=n-square; 2=quadtree; 3=octree
  NEIGHBOR_SEARCH_N     = 10
  MN                    = 20       ! Maximum number of neighbors allowed per particle
  KN = 1.0d+6               ! Normal inter-particle collision spring constant
  KT_FAC = @(2/7)           ! Tangential spring factor = KT/KN
  MEW = 0.0                 ! Inter-particle friction coeffienct
  KN_W = 1.0d+6             ! Normal particle-wall collision spring constant
