########################################################################
#  Author: J.Musser                                 Date:   2012-11-15 #
#                                                                      #
#  Purpose: Test the DEM particle-particle contact conduction model.   #
#                                                                      #
#  REF: Musser, J., “Modeling of heat transfer and reactive chemistry  #
#       for particles in gas-solid flow utilizing continuum-discrete   #
#       methodology (CDM),” WVU, PhD Thesis 2011.                      #
#                                                                      #
########################################################################

  RUN_NAME = 'Qpp_VV' 
  DESCRIPTION = 'two stacked particles with contact conduction'


#_______________________________________________________________________
# RUN CONTROL SECTION

  RUN_TYPE = 'NEW'
  UNITS = 'CGS'

  TIME =    0.0
  TSTOP = 500.0

  DT = 1.0d-4

  ENERGY_EQ = .T.
  SPECIES_EQ(0:2) = .F. .F. .F.

  MOMENTUM_X_EQ = .F. .F. .F.
  MOMENTUM_Y_EQ = .F. .F. .F.
  MOMENTUM_Z_EQ = .F. .F. .F.

  DRAG_TYPE = 'WEN_YU'

  CALL_USR = .T.

! The solids time step is forced to C(1).
  C(1) = 1.0d-3   C_NAME(1) = 'Forced DTSOLID'

  DES_CONTINUUM_COUPLED = .F.
  DES_INTERP_ON = .F.

#_______________________________________________________________________
# NUMERICAL SECTION


#_______________________________________________________________________
# GEOMETRY SECTION 

  COORDINATES = 'Cartesian'

  XLENGTH  =  4.50   IMAX =  3
  YLENGTH  = 15.00   JMAX = 15
  ZLENGTH  =  0.36   NO_K = .T.


! Grid partition for DES grid
  DESGRIDSEARCH_IMAX = 1
  DESGRIDSEARCH_JMAX = 1
  DESGRIDSEARCH_KMAX = 1


#_______________________________________________________________________
# MATERIAL SECTION


! GAS SECTION
!---------------------------------------------------------------------//
  RO_g0 = 1.3E-3       ! incompressible
  MU_g0 = 1.8E-4    

  C_Pg0 = 2.3878E-1  ! cal/(g.K)

!---------------------------------------------------------------------//
  MMAX = 2

  PARTICLES = 2   ! Number of partiles in the system.

! Particle-particle:
  KN =   1.6d7                 ! Normal collision spring constant
  MEW =  0.0                   ! Friction coefficient

! Particle-wall
  KN_W =  1.6d7                ! Normal collision spring constant
  MEW_W = 0.0                  ! Friction coefficient

! Restitution coefficients

  DES_EN_INPUT(1:2) = 0.9  0.9 ! e11  e12
  DES_EN_INPUT(3)   =      0.9 !      e22

  DES_EN_WALL_INPUT = 0.9  0.9 ! e1w  e2w

  DES_NEIGHBOR_SEARCH = 4          ! Grid based neighbor search
  NEIGHBOR_SEARCH_N   = 9999   ! Steps between neighbor search
  MN = 20                      ! Maximum number of neighbors


! Solids phase 1 :: Cold particle
!---------------------------------------------------------------------//
  SOLIDS_MODEL(1) = 'DEM'

  D_p0(1) =   0.30      ! (cm)
  RO_s0(1) =  0.42      ! (g/cm^3)

  K_s0(1) =   0.131453  ! Conductivity  :: cal/(sec.cm.K)
  C_ps0(1) =  0.191204  ! Specific Heat :: cal/(g.K)
  DES_Em(1) = 0.0       ! Emmisivity



! Solids phase 2 :: Hot particle
!---------------------------------------------------------------------//
  SOLIDS_MODEL(2) = 'DEM'

  D_p0(2) =   0.36      ! (cm)
  RO_s0(2) =  8.85      ! (g/cm^3)

  K_s0(2) =   0.002008  ! Conductivity  :: cal/(sec.cm.K)
  C_ps0(2) =  0.013145  ! Specific Heat :: cal/(g.K)
  DES_Em(2) = 0.0       ! Emmisivity



#_______________________________________________________________________
# INITIAL CONDITIONS SECTION

  IC_X_w(1) =      0.0           
  IC_X_e(1) =      4.5           
  IC_Y_s(1) =      0.0           
  IC_Y_n(1) =     15.0          

  IC_EP_g(1) =     1.0           
 
  IC_U_g(1) =      0.0           
  IC_V_g(1) =      0.0
  IC_W_g(1) =      0.0           

  IC_P_g(1) =      0.0           
  IC_T_g(1) =    273.0

  IC_T_s(1,1) =  298.15 ! Temperature of cold particle (K) -> 25C
  IC_T_s(1,2) =  453.15 ! Temperature of hot particle (K) -> 180C


#_______________________________________________________________________
# BOUNDARY CONDITIONS SECTION

! No BCs needed for this case.


#_______________________________________________________________________
# OUTPUT CONROL SECTION

  RES_DT = 10.0     ! interval at which restart (.RES) file is updated
  NLOG = 25          ! interval in no. of time steps at which (.LOG) file is written


! Interval at which .SPX files are written 
  SPX_DT(1)  = 10.0     ! Void fraction (EP_g)
  SPX_DT(2)  = 10000.0     ! Gas and solids pressure (P_g, P_star)
  SPX_DT(3)  = 10000.0     ! Gas velocity (U_g, V_g, W_g)
  SPX_DT(4)  = 10000.0     ! Solids velocity (U_s, V_s, W_s)
  SPX_DT(5)  = 10000.0     ! Solids density (ROP_s)
  SPX_DT(6)  = 10000.0     ! Gas and solids temp. (T_g, T_s1, T_s2)
  SPX_DT(7)  = 10000.0     ! Gas and solids mass fractions (X_g, X_s)
  SPX_DT(8)  = 10000.0     ! Granular Temperature (G)
  SPX_DT(9)  = 10000.0     ! User defined scalars

  FULL_LOG = .T.               ! display residuals on screen
  RESID_STRING = 'P0' 'U0' 'V0'   ! which residuals to display


# DMP control
# -----------------------------
  NODESI = 1 
  NODESJ = 1
  NODESK = 1



  PRINT_DES_DATA = .FALSE. ! write DEM data : *.vtp files
  DES_RES_DT = 10.0       
  DES_SPX_DT = 10.0       ! interval to write *.vtp files in pure DEM simulation
