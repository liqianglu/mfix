# This is not a real run; Certain values are printed out from rrates to test the reading
# of thermo properties and heat of reaction calculation; then the code is made to exit.
# Data are read from this file as well as from mfix/model/thermochemical/BURCAT.THR

  RUN_NAME = 'Thermo test'
  DESCRIPTION = 'Test the reading of thermochemical database.'

  NMAX(0)= 5
  SPECIES_NAME = 'CH4'  'CO2'   'O2'  'H2O'  'N2'

  MMAX = 2
  NMAX(1)= 5
  NMAX(2)= 4
  SPECIES_NAME(6) = 'AL2O3'   'ZnCl2'  'Fe(OH)2'  'Fixed Carbon' 
  SPECIES_NAME(10) = 'Volatile Matter'
  SPECIES_NAME(11) = 'SiS2  Solid'  'C6H11  2M-4en3yl'   'SiHCL3' 'SiH4  Silane'

  MW_s(1,5) = 63  !override the molecular weight read from the database



! Most of the rest of the datafile is to get MFIX not to quit before rrates.

  RUN_TYPE = 'new'
  UNITS = 'cgs'
  TIME  = 0.0    TSTOP = 25.    DT = 1.0E-4 
  ENERGY_EQ = .TRUE.
  SPECIES_EQ = .TRUE.    .FALSE.

  DEF_COR = .true.
  MAX_NIT = 100
  
  Norm_g = 0.
  
  C(1) = 1.0E10  ! reaction rate coefficient

#
# Geometry Section
#
  COORDINATES = 'Cartesian'

  XLENGTH  =  10.   IMAX = 1
  YLENGTH  =  2.   JMAX = 10  
  NO_K = .true.
  ZLENGTH  =  10.   KMAX = 1 

#
# Gas-phase Section
#
#

  MU_g0 = 1.8E-4
  Dif_g0 = 0.

#
# Solids-phase Section
#

  C_e = 0.8  C_f = 0.3  Phi = 30.   EP_star = 0.4
  D_p0 = 1  1
  RO_s = 1  1
#
# Initial Conditions Section
#
       !              
  IC_X_w           =  0.0      
  IC_X_e           =  10.0 
  IC_Y_s           =  0.0  
  IC_Y_n           =  2. 
 
  IC_EP_g          =  1.0
  IC_T_g	   = 1000.
 
  IC_U_g           = 0.0   
  IC_V_g           = 0.0
  IC_X_g(1,1)      = 0.0463    !1 mole CH4
  IC_X_g(1,3)      = 0.2222     !2.4 moles O2
  IC_X_g(1,5)      = 0.7315    !9.03 moles N2

#
#  Boundary Conditions Section
#
       !             Inlet       Outlet
  BC_X_w           =  0.0        0.0
  BC_X_e           =  10.0       10.0 
  BC_Y_s           =  0.0       2.00
  BC_Y_n           =  0.0       2.00
 
  BC_TYPE          = 'MI'        'PO'

  BC_EP_g          =  1.0   

  BC_U_g           =  0.0    
  BC_V_g           =  10.

  BC_P_g           = 1.013E6       1.013E6

  BC_T_g           = 298.         

  BC_X_g(1,1)      = 0.0463    !1 mole CH4
  BC_X_g(1,3)      = 0.2222     !2.4 moles O2
  BC_X_g(1,5)      = 0.7315    !9.03 moles N2


#
#  Output Control
#
  out_dt = 1.
  RES_DT = 0.01
        !
        ! EP_g P_g       U_g  U_s  ROP_s     T_g  X_g    Theta_m  Scalar
        !      P_star    V_g  V_s            T_s1 X_s
        !                W_g  W_s            T_s2
  SPX_DT = 100. 100.     0.1  100.  100.      .1   .1   100.      100.

  FULL_LOG = .true.
  NLOG   = 25
  
    RESID_STRING = 'P0' 'V0' 'T0' 'T1' 'X001'


