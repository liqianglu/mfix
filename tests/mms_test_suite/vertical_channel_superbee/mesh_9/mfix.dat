 ! Horizontal Channel Simulation                
 ! Run-control section                                                 
 
   RUN_NAME              = 'VCS'           
   DESCRIPTION           = 'Gas flow in a vertical channel - Superbee'
   RUN_TYPE              = 'new'             
   UNITS                 = 'SI'
   TIME                  = 0.0                !start time
!   TSTOP                 = 10.0
!   DT                    = 1.0E-3             !No time step specified --> Steady state solution
   ENERGY_EQ             = .FALSE.            !do not solve energy eq
   SPECIES_EQ(0)            = .FALSE.  	      !do not solve species eq
   MAX_NIT 		 = 50000 

   MMAX                  = 0

   GRAVITY               = 0.0

!   MOMENTUM_X_EQ(1) = .FALSE.
!   MOMENTUM_Y_EQ(1) = .FALSE.

   CALL_USR = .TRUE.

   NORM_g = 0.0d0
