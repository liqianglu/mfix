!  Top wall driven fluid-solid driven cavity Simulation 
!  DES for solid phase and Kinetic theory of fluid phase
!  with cohesion


! For more details refer to PhD thesis (Dec 2003) of Jay Boyalakuntla
! For more details refer to PhD thesis (2004) of Michael Weber
! It is made available in MFIX documents                                     
                           
! Run-control section                                                           
  RUN_NAME 		= 'DES_DRIV_CAV_VDW'             
  DESCRIPTION 		= 'Top wall driven cavity with solid particles in the container'          
  RUN_TYPE 		= 'new'             
  UNITS 		= 'cgs'
  TIME  		= 0.0			!start time
  TSTOP 		= 0.5 
  DT 			= 1E-4			!time step
  DT_MAX                = 7.5E-4
 
  ENERGY_EQ 		= .FALSE.		!do not solve energy eq
  SPECIES_EQ 		= .FALSE.    .FALSE. 	!do not solve species eq

  MOMENTUM_X_EQ(1) = .FALSE.
  MOMENTUM_Y_EQ(1) = .FALSE.
  MOMENTUM_Z_EQ(1) = .FALSE.

!  NORM_G = 1.0

  MAX_NIT = 2500
  TOL_RESID = 1.E-5
                
                                                               
! Geometry Section                                                      
                                                                         
  COORDINATES 		= 'cartesian' 
  XLENGTH  		=   3.0	        !X length	
  IMAX 			=   15			!cells in i direction
  YLENGTH  		=   3.0		!height
  JMAX 			=   15 			!cells in j direction
  DZ(1)                 =   0.1
  NO_K     		= .TRUE. 		!2D, no k direction

  GRAVITY = 0
 
   ur_fac(2) = 0.8
   
   ur_fac(1) = 0.8                          !pressure
   
   ur_fac(3) = 0.8                          !u-velocity
   
   ur_fac(4) = 0.8                          !v-velocity

                                                                             
! Gas-phase Section                                                             
 
  MU_g0 		= 2E-3		!constant gas viscosity
  RO_g0			= 1.205E-2		!constant gas density

 
! Solids-phase Section                                                  


  RO_s    		= 0.75 			!solids density
  D_p0     		= 0.1			!particle diameter
  e       		= 0.9 			!restitution coefficient
  Phi    		= 30.0			!angle of internal friction
  EP_star 		= 0.42			!void fraction at minimum
						!  fluidization

  
! Initial Conditions Section                                                    

    ! 1. bed
  IC_X_w           	=  0.0				!domain
  IC_X_e           	=  3.0		! 0 < x < 15, 0 < y < 30
  IC_Y_s          	=  0.0
  IC_Y_n           	=  3.0
                                	! initial values in the region
  IC_EP_g          	=  1.0		! void fraction
  IC_U_g           	=  0.0		! x-dir gas velocity
  IC_V_g          	=  0.0	! y-dir gas velocity

  IC_P_g           =  0.0

  IC_U_s(1,1) = 0.0
  IC_V_s(1,1) = 0.0

!  Boundary Conditions Section

      ! 1. Central jet
  BC_X_w(4)		=  0.0		! central jet
  BC_X_e(4)		=  3.0		! 7 < x < 8, y = 0
  BC_Y_s(4)		=  3.0
  BC_Y_n(4)		=  3.0
  BC_TYPE(4)		= 'PSW'		!specified mass inflow

  BC_Uw_g(4)       = 200.0
  BC_Vw_g(4)       = 0.0

  BC_Uw_s(4, 1)    = 0.0
  BC_Vw_s(4, 1)    = 0.0

!
! DES INPUT

! Begin
! Parameters

PARTICLES = 49
MN = 8

KN = 800000
KT = 800000
ETA_DES_N = 18
ETA_DES_T = 18
MEW = 0.2
KN_W = 1200000
KT_W = 1200000
ETA_N_W = 22
ETA_T_W = 22
MEW_W = 0.1
DES_NEIGHBOR_SEARCH = 1
DTSOLID_FACTOR = 0.1D0
P_TIME = 10.0
PRINT_DES_DATA = .TRUE.
NFACTOR = 0

USE_COHESION=.TRUE.
SQUARE_WELL=.FALSE.
MASTER_WELL_DEPTH = 0.1
MASTER_WALL_WELL_DEPTH=0.0
RADIUS_RATIO=1.1
WALL_RADIUS_RATIO=1.1
VAN_DER_WAALS=.TRUE.
HAMAKER_CONSTANT = 1e-10
WALL_HAMAKER_CONSTANT = 0.0
VDW_INNER_CUTOFF=4e-8
WALL_VDW_INNER_CUTOFF=4e-8
VDW_OUTER_CUTOFF=0.01
WALL_VDW_OUTER_CUTOFF=0.01
COHESION_DEBUG=0
FOCUS_PARTICLE=4004

!  DES Logicals
DISCRETE_ELEMENT = .TRUE.
WALLDTSPLIT = .TRUE.
DES_CONTINUUM_COUPLED = .TRUE.
TSUJI_DRAG = .TRUE.

! End DES Input

! Output Control
!
  OUT_DT 		= 0.05		!write text file BUB01.OUT
					!  every 0.1s
  RES_DT 		= 0.01		!write binary restart file
					!  BUB01.RES every 0.01 s
  NLOG   		= 25 		!write logfile BUB01.LOG
					!every 25 time steps
  FULL_LOG 		= .TRUE. 	!display residuals on screen


!SPX_DT values determine how often SPx files are written.  Here BUB01.SP1, which
!contains void fraction (EP_g), is written every 0.01 s, BUB01.SP2, which contains
! gas and solids pressure (P_g, P_star), is written every 0.1 s, and so forth.
!
        ! EP_g P_g       U_g  U_s  ROP_s     T_g  X_g
        !      P_star    V_g  V_s            T_s  X_s     Theta   Scalar 
        !                W_g  W_s
  SPX_DT = 0.01 0.1   0.1  0.1  100.  100. 100.   100.0   100. 100.  100.        
                                                                                
!  The decomposition in I, J, and K directions for a Distributed Memory Parallel machine
  
  NODESI = 1   NODESJ = 1   NODESK = 1

!  Sweep Direction

LEQ_SWEEP(1) = 'ISIS' 
LEQ_SWEEP(2) = 'ISIS'  
LEQ_SWEEP(3) = 'ISIS' 
LEQ_SWEEP(4) = 'ISIS'  
LEQ_SWEEP(5) = 'ISIS'  
LEQ_SWEEP(6) = 'ISIS'  
LEQ_SWEEP(7) = 'ISIS' 
LEQ_SWEEP(8) = 'ISIS'  
LEQ_SWEEP(9) = 'ISIS'

